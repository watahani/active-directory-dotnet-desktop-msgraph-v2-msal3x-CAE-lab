<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <system.diagnostics>
      <trace autoflush="true" indentsize="4">
        <listeners>
          <add name="myListener"
                    type="System.Diagnostics.TextWriterTraceListener"
                    initializeData=".\LogData.log" />
          <remove name="Default" />
        </listeners>
      </trace>
    </system.diagnostics>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/>
    </startup>
</configuration>
